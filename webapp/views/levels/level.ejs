<%include ../partials/header.ejs%>

<div class="container level-container">
    <h1 class="level-title">Level <%=level%></h1>
    <div class="image-parent">
        <img class="image-responsive" src="/images/l<%=level%>.png">
        <%if (level == 2) {%>
            <img class="mike image-child" style="left: 65%; top: 30%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 23%; top: 10%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 3) {%>
            <img class="mike image-child" style="left: 70%; top: 40%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 20%; top: 45%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 4) {%>
            <img class="mike image-child" style="left: 65%; top: 55%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 25%; top: 40%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 5) {%>
            <img class="mike image-child" style="left: 55%; top: 50%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 6) {%>
            <img class="mike image-child" style="left: 47%; top: 50%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 75%; top: 55%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 7) {%>
            <img class="mike image-child" style="left: 47%; top: 50%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 75%; top: 55%;"src ="/images/smalllogo.png"/>
        <%}%>
        <%if (level == 8) {%>
            <img class="mike image-child" style="left: 45%; top: 45%;"src ="/images/smalllogo.png"/>
            <img class="mike image-child" style="left: 70%; top: 30%;"src ="/images/smalllogo.png"/>
        <%}%>
    </div>
    <div id="myChart" style="width:100%; height:400px;"></div>

</div>

