<%include ../partials/header.ejs%>

<div class="container level-container">
    <h1 class="level-title">Level <%=level%></h1>
    <div class="image-parent">
        <img class="image-responsive" src="/images/tables.png">
        <img class="image-child mike" id="sensor0Aicon" style="left: 71%; top: 75%;" src ="/images/smalllogo.png"/>
        <div class="sensorText" style="left: 76%; top: 72%;">
            <p><strong>Sensor 0A</strong></p>
            <p><strong>Current value: </strong><span class="sensorTextVal"></span></p>
            <p><em>Click icon for more info below.</em></p>
        </div>
        <img class="image-child mike" id="sensor0Bicon" style="left: 23%; top: 15%;" src ="/images/smalllogo.png"/>
        <div class="sensorText" style="left: 28%; top: 12%;">
            <p><strong>Sensor 0B</strong></p>
            <p><strong>Current value: </strong><span class="sensorTextVal"></span></p>
            <p><em>Click icon for more info below.</em></p>
        </div>
    </div>
    <div id="myChart0A" class="graphs" style="width:100%; height:400px;"></div>
    <div id="myChart0B" class="graphs" style="width:100%; height:400px;"></div>
</div>

<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="/javascript/graphs.js" level="<%=level%>" id="graph"></script>
<script src="/javascript/indicator.js" level="<%=level%>"></script>
<script src="/javascript/notifications.js" level="<%=level%>"></script>
<%include ../partials/footer.ejs%>